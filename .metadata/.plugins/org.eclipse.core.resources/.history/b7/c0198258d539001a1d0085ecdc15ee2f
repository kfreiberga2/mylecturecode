
public class Calculator {

	
	public static void main(String[] args) {
		
		char operatorMath = '*';
		double num1 = 10;
		double num2 = 3;
		double result;
		
		
		
		if(operatorMath=='+'){
			result = num1+num2
			System.out.println(num1+num2);
			
		}else if(operatorMath=='/'){
			System.out.println(num1/num2);
		}else if(operatorMath=='*'){
			System.out.println(num1*num2);
		}else if(operatorMath=='%'){
				System.out.println(num1%num2);
			
		}else{
			System.out.println("wrong data");
		}

	}

}
