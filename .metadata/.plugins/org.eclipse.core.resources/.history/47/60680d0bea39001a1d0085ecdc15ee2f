
public class ForLoopAndIfElseBlock {

	
	public static void main(String[] args) {
		
		for (int i=0; i<10; i++){
			if(i==4){
				System.out.println("You find number i: "+i);
				break;
			}
			System.out.println(i);
			
			for (int i=0; i<10; i++){
				if(i==4){
					System.out.println("You find number i: "+i);
					break;
				}
				System.out.println(i);
		}

	}

}
