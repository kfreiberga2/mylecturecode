package lv.kristine;

import java.util.ArrayList;

public class TaskTestArrayMethods {

	public static void main(String[] args) {
		ArrayList<String> cars = new ArrayList<>();

		cars.add("BMW");
		cars.add("Audi");
		cars.add("Mazda");

		for (String c : cars) {
			System.out.println(c);
		}

		System.out.println(cars.size());
		cars.remove("Audi");
		
		for (String c : cars) {
			System.out.println(c);
		}
	}

}
