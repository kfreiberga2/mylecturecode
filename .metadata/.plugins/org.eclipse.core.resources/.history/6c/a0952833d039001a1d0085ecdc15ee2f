
public class PrintNumberInWord {

	public static void main(String[] args) {
	
		// using equal operator == to check if value is true
		
		int number = 2;
		
		
		if (number==1){
			System.out.println("ONE");
		}else if(number==2){
			System.out.println("TWO");
		}else(number==3);{
			System.out.println("THREE");
		}

			
			}
	
		
	}

