
public class ArraysSumForLoop {

	
	public static void main(String[] args) {
		int [] marks = {76,23,56,11,78,28,88};
		// sum all values in arrays 
		// print to console
		
		int result = 10;
		System.out.println("result value:" +result);

		int testInt = 90;
		result = testInt;
		System.out.println("result value:" +result);
		System.out.println(marks[0]+marks[1]);
		
	}

}
