public class TestIfElseAndBlocks {

	public static void main(String[] args) {

		System.out.println("==========");
		System.out.println("==========");

		
		boolean test = true;
		
		if (test){
			System.out.println("TRUE");
		}else{
			System.out.println("FALSE");
		}
		System.out.println("After if elese block");
		
		boolean test2 = false;
		

	}

}
