
public class ControlFlowIf {

	
	public static void main(String[] args) {
		int x = 10;
		int y = 22;
		
		if (x<y){
			System.out.println("X<Y = pareizi"); 
			//ja tas ir true, tad paradisies println teksts
		}
		
		
		int numberCheck= 21;
		if(numberCheck%2 == 0){ //1==0 return false
			System.out.println(numberCheck+ " is even-paara");
		}else{
			System.out.println(numberCheck +" is odd-nepaara");
		}
		
		int mark = 13;
		if (mark>=50){
		System.out.println("True");
		}else{
		System.out.println("False");	
		}
		
		

	}

}

