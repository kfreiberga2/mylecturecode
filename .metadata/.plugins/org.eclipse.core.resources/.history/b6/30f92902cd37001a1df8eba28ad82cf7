
public class TaskVar {

	/**
	 * 
125 + 24 = 149
125 - 24 = 101
125 x 24 = 3000
125 / 24 = 5
125 mod 24 = 5

	 */
	public static void main(String[] args) {
		int numAdd;
		numAdd=125+24;
		System.out.println(numAdd+ "=125+24");
		int numSubstract=125-24;
		System.out.println(numSubstract+ "=125-24");
		int num1 =125;
		int num2= 24;
		System.out.println(num1+"x"+"num2"+"="+(num1*num2));
		
		
				
	}

}
