package lv.kristine;

public class LearnMethods {

	public static void main(String[] args) {
		double area = getArea();
		System.out.println("Area of rectangle is :"+getArea());
	}

	
	
	public static double getArea() {
		double num1=5.0;
		double num2=20.3;
		double result = num1*num2;				
		return result;
		
	}

}
