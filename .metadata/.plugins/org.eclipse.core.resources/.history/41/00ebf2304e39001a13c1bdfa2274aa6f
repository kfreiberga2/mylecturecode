
public class PrimType {

	public static void main(String[] args) {
		
		// numeric data primitive type
		int integer = 2;
		long longInteger = 92837525398L;
		short shortInteger = 754;
		byte byteInteger = 56;
		
		//Floating-Point tyoe real numbers
		double doubleFloutNumber = 0.5d;
		float floatNumber = 0.932857f; 
		
		//characters
		char charSymbol = 'a';
		char charSymbolAsNumber = 33;
		char charK = 75;
		char cR = 82;
		char cI = 73;
		char cS = 83;
		System.out.println(charK);
		
		
		System.out.println("Char: " +charSymbolAsNumber);
		
		
	}

}
